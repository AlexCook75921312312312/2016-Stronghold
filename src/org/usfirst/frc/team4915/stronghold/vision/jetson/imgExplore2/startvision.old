#!/bin/bash -f

visdir="$HOME/2016-Stronghold/src/org/usfirst/frc/team4915/stronghold/vision"

if [ ! -d $visdir ] ; then
    visdir="$HOME/vision/jetson"
fi

if [ -d $visdir ] ; then
   cd $visdir
   cd jetson/imgExplore2
   echo "starting imgServer.py"
   ./imgServer.py >& /var/tmp/imgServer.log &

   echo "starting imgExplore.py"
   python ./imgExplore.py --algorithm 5 --stashinterval 1 --nodisplay &
else
   echo "can't find $visdir"
fi
